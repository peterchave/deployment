server {
	listen 80 default_server;
	listen [::]:80 default_server;
        
	root /var/www/html;

	index index.html index.htm index.nginx-debian.html;

	server_name _;
	
	#IN#
	location / {
		try_files $uri $uri/ =404;
	}
	#OUT#
} 

map $http_upgrade $connection_upgrade {
        default upgrade;
        '' close;
}
